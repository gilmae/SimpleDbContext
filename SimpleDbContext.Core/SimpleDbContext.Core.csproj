  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)|$(Platform)' == 'Release|AnyCPU' ">
    <OutputPath>..\SimpleDbContext.Core\bin\Release</OutputPath>
  </PropertyGroup>
  <ItemGroup>

  </ItemGroup>
</Project>
